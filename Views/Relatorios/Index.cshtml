@model IEnumerable<ApiInfoDengue.Domain.Relatorio>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataSolicitacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Arbovirose)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SemanaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SemanaFim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoIBGE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Municipio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Solicitante)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataSolicitacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Arbovirose)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SemanaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SemanaFim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoIBGE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Municipio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Solicitante.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
